   "source": [
    "s1 = input()\n",
    "s2 = input()\n",
    "if len(s1) > len(s2):\n",
    "    s1, s2 = s2, s1\n",
    "sonlst = []\n",
    "for i in range(len(s1)):\n",
    "    for j in range(i+1,len(s1)+1):\n",
    "        if s1[i:j] in s2:\n",
    "            sonlst.append(s1[i:j])\n",
    "if sonlst:\n",
    "    print(len(max(sonlst,key=len)))\n",
    "else:\n",
    "    print(0)"
