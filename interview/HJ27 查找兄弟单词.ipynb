   "source": [
    "lst = list(input().split())\n",
    "n = int(lst[0])\n",
    "dct = lst[1:n+1]\n",
    "x = lst[n+1]\n",
    "k = int(lst[-1])\n",
    "xlst = []\n",
    "for item in dct:\n",
    "    if sorted(item) == sorted(x) and item != x:\n",
    "        xlst.append(item)\n",
    "print(len(xlst))\n",
    "if len(xlst) - k > 0:\n",
    "    print(sorted(xlst)[k-1])"
   ]
