   "source": [
    "n, m = map(int, input().split())\n",
    "def fn(x ,y):\n",
    "    if x < 0 or y < 0:\n",
    "        return 0\n",
    "    elif x == 0 or y == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return fn(x-1, y) + fn(x, y-1)\n",
    "\n",
    "print(fn(n, m)) \n"
   ]
